<section class="view-content">

{{gh-edit-client
    isEditing=isEditing
    name=model.name
    status=model.status
    description=model.description
    secret=model.secret
    logo=model.logo
    redirection_uri=model.redirection_uri
    changeClientStatus="changeClientStatus"
    refreshSecret="refreshSecret"
    saveClient=(action 'saveClient')
}}
</section>
